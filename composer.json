{
    "name": "lucaslz/easyentry",
    "description": "Framework criado para trabalho de Engenharia de Software 2",
    "keywords": ["microframework", "rest", "router", "psr7"],
    "homepage": "http://github.com/lucaslz/easyentry",
    "license": "MIT",
    "authors": [
        {
            "name": "Lucas Lima",
            "email": "lucaslz@rocketmail.com"
        }
    ],
    "platform": {
        "php": "5.6"
    },
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "firebase/php-jwt": "5.0.0",
        "illuminate/database": "5.4.36",
        "robmorgan/phinx": "^0.9.2",
        "tuupola/slim-jwt-auth": "^3.0",
        "slim/twig-view": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/",
            "Core\\": "src/Core/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout" : 0
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }

}
