<h4 class="text-muted text-center">Dados de Usuario!</h4>
<div class="form-group row">
    <label for="email" class="col-sm-4 col-form-label">Email</label>
    <div class="col-sm-8">
        <input type="text" value="{{ dados.email }}" class="form-control" name="email">
    </div>
</div>
<div class="form-group row">
    <label for="senha" class="col-sm-4 col-form-label">Senha</label>
    <div class="col-sm-8">
        <input type="password" value="{{ dados.senha }}" class="form-control" name="senha">
    </div>
</div>
<div class="form-group row">
    <label for="confirmarSenha" class="col-sm-4 col-form-label">Confirmar Senha</label>
    <div class="col-sm-8">
        <input type="password" value="{{ dados.confirmar_senha }}" class="form-control" name="confirmar_senha">
    </div>
</div>
<div class="form-group row">
    <label for="tipoUsuario" class="col-sm-4 col-form-label">Tipo Usuario</label>
    <div class="col-sm-8">
        <select name="tipoUsuario" class="custom-select">
            <option>Selecione</option>
            {% for usuario in tipoUsuario %}
                {% if loop.index == dados.tipoUsuario %}
                    <option value="{{ loop.index }}" selected>{{ usuario }}</option>
                {% else %}
                    <option value="{{ loop.index }}">{{ usuario }}</option>
                {% endif %}
            {% endfor %}
        </select>
    </div>
</div>
